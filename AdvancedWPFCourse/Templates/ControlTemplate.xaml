<Window x:Class="WpfTrainingApp.Templates.ControlTemplate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfTrainingApp.Templates"
        mc:Ignorable="d"
        Title="ControlTemplate" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button x:Name="btnOff" Style="{StaticResource RoundRedButton}" Content="Off" Margin="20"  Click="BtnOff_OnClick"></Button>
        </StackPanel>
    </Grid>
</Window>
